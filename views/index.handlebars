        <section>
            <nav>
                <ul class="menu">
                    <li>Home</li>
                    <li>Our Food</li>
                    <li>Our Menu</li>
                    <li class="logo">Eat De Burger</li>
                    <li>Gallery</li>
                    <li>Locate</li>
                    <li>Contact</li>
                </ul>
            </nav>
            <div class="big-font">
                Fresh &
                <div class="burger-img"></div>
                Tasty
            </div>
            <div class="order-container">
                <div class="order-left-container">
                    {{#each burger_data}}
                    {{#unless this.devoured}}
                    <div data-index="{{this.id}}" class="burger">
                        {{this.id}}. {{this.burger_name}}<span class="button-remove">
                            <!-- prettier-ignore -->
                            <svg class="eat-icon" preserveAspectRatio="none" viewBox="0 0 49 49" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path d="M16.0781 4.05811V12.8807" stroke="#F4F4F4" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                <path d="M16.0781 22.5856V46.4067" stroke="#F4F4F4" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                <path d="M21.4375 4.05811L22.9688 14.6453C22.9687 16.7512 22.2428 18.7708 20.9505 20.26C19.6583 21.7491 17.9056 22.5856 16.0781 22.5856C14.2506 22.5856 12.498 21.7491 11.2057 20.26C9.91347 18.7708 9.1875 16.7512 9.1875 14.6453L10.7188 4.05811" stroke="#F4F4F4" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                <path d="M39.0469 32.2905H28.3281C28.3281 32.2905 30.625 7.58716 39.0469 4.05811V46.4067" stroke="#F4F4F4" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                </svg>
                        </span>
                    </div>
                    {{/unless}}
                    {{/each}}
                </div>
                <div class="order-right-container">
                    <div class="burger-devoured" data-index="uuid">1. Deluxe Burger</div>
                </div>
            </div>
            <hr />
            <div class="submit-order-container">
                <input
                    type="text"
                    class="input-order"
                    placeholder='Enter a burger you want to eat Click the "Submit" button.'
                />
                <input type="submit" class="submit" />
            </div>
        </section>